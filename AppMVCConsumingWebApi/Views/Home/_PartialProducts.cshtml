@model IEnumerable<AppMVCConsumingWebApi.Models.Product>
@{
      Layout = null;

}
<table class="table table-condensed table-hover table-striped">
  <thead>
    <tr>
      <th data-column-id="id" data-type="numeric">ID</th>
      <th data-column-id="sender">Name</th>
      <th data-column-id="received" data-order="desc">Price</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var item in Model)
    {
      <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.Price</td>
    </tr>
    }
  </tbody>
</table>